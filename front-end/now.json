{  "version": 2,
    "alias":["eth4top.now.sh"],
    "routes": [
        {
          "src": "/(.*)\\.(.*)",
          "dest": "/build/$1.$2"
        },
        {
          "src": "/",
          "dest": "/build/index.html"
        },
        {
          "src": "/(.*)",
          "status": 301, "headers": { "Location": "/" }
        }
      ]
}